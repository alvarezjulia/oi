{{!-- <h1>{{title}}</h1>
<p>Welcome to {{title}}</p> --}}
<div class="filter-button-wrapper">
  <button class="btn btn-primary" type="button" data-toggle="collapse" data-target="#collapseExample"
    aria-expanded="false" aria-controls="collapseExample">
    Filter locations
  </button>
</div>
<div class="collapse" id="collapseExample">
  <div class="filter-locations-wrapper">
    <form action="/" method="post">
      {{#each locationNames}}
      <div class="custom-control custom-checkbox" id="filter-location-box">
        <input name ="filter-check{{this._id}}" type="checkbox" class="custom-control-input" id="customCheck{{this._id}}" value="{{this.name}}">
        <label class="custom-control-label" for="customCheck{{this._id}}">{{this.name}}</label>
      </div>
      {{/each}}
      <button type="submit">Filter!</button>
    </form>
  </div>
</div>

{{#each events}}
<div class="event-container" id="{{_id}}">
  <div class="location">{{this.location}}</div>
  <div class="event">{{this.event}}</div>
  <div class="info-wrapper">
    <div class="door">door: {{door}}</div>
    <div class="begin">begin: {{begin}}</div>
    <div class="end">end: {{end}}</div>
    <div class="price">price: {{price}}</div>
  </div>
</div>
{{/each}}